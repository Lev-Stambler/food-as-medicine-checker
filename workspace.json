{
  "version": 1,
  "projects": {
    "health-site-scraper": {
      "root": "apps/health-site-scraper",
      "sourceRoot": "apps/health-site-scraper/src",
      "projectType": "application",
      "prefix": "health-site-scraper",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/health-site-scraper",
            "main": "apps/health-site-scraper/src/main.ts",
            "tsConfig": "apps/health-site-scraper/tsconfig.app.json",
            "assets": ["apps/health-site-scraper/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/health-site-scraper/src/environments/environment.ts",
                  "with": "apps/health-site-scraper/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "health-site-scraper:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/health-site-scraper/.eslintrc",
            "tsConfig": [
              "apps/health-site-scraper/tsconfig.app.json",
              "apps/health-site-scraper/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/health-site-scraper/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/health-site-scraper/jest.config.js",
            "tsConfig": "apps/health-site-scraper/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "scholars-scraper": {
      "root": "apps/scholars-scraper",
      "sourceRoot": "apps/scholars-scraper/src",
      "projectType": "application",
      "prefix": "scholars-scraper",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
